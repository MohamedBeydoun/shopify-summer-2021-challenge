    "start:prod": "NODE_ENV=production node dist/server.js",
    "build": "tsc -p tsconfig.json",
    "watch": "tsc -w",
    "format": "eslint --fix --max-warnings 0 '{src,tests}/**/*.{js,ts}'",
    "lint": "eslint --max-warnings 0 '{src,tests}/**/*.{js,ts}'"
  },
